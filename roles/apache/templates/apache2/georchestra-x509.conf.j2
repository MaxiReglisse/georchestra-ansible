<VirtualHost *:80>
     ServerName {{ georchestra.fqdn }}
     DocumentRoot /var/www/georchestra/htdocs
     LogLevel warn
     ErrorLog /var/www/georchestra/logs/error.log
     CustomLog /var/www/georchestra/logs/access.log "combined"
     Include /var/www/georchestra/conf/*.conf
     ServerSignature Off
</VirtualHost>
<VirtualHost *:443>
     ServerName {{ georchestra.fqdn }}
     DocumentRoot /var/www/georchestra/htdocs
     LogLevel warn
     ErrorLog /var/www/georchestra/logs/error.log
     CustomLog /var/www/georchestra/logs/access.log "combined"
     Include /var/www/georchestra/conf/*.conf
     SSLEngine On
     SSLCertificateFile /var/www/georchestra/ssl/x509/cert-{{ x509.certnumber }}-{{ georchestra.fqdn }}.pem
     SSLCertificateKeyFile /var/www/georchestra/ssl/x509/{{ georchestra.fqdn }}.key
     SSLCACertificateFile /var/www/georchestra/ssl/x509/chain-{{ x509.certnumber }}-{{ georchestra.fqdn }}.pem
     ServerSignature Off
</VirtualHost>
